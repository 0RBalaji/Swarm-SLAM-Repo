      <sensor name='laser' type='ray'>
        <visualize>0</visualize>
        <update_rate>10</update_rate>
        <pose> 0 0 0.02 0 0 0</pose>
        <ray>
          <scan>
            <horizontal>
              <samples>360</samples>
              <min_angle>-3.14</min_angle>
              <max_angle>0</max_angle>
              <!-- <resolution>1</resolution> -->
